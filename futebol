let selectedDayI = Math.max(0, dias.findIndex(d => d.classList.contains("active")));if (selectedDayI < 0) selectedDayI = todayDayIndex;// ✅ modo: 'day' (calendário) OU 'range' (Hoje/Amanhã/Semana/Tudo)
let mode = 'range';// range atual
let range = 'today'; // today/tomorrow/week/all
function scrollToIndex(index){const day = dias[index];if(!day || !diasContainer) return;const left = day.offsetLeft - (diasContainer.clientWidth/2) + (day.clientWidth/2);diasContainer.scrollTo({left, behavior:"smooth"});}function dayInRange(dayI){dayI = parseInt(dayI, 10);if (Number.isNaN(dayI)) return false;if (range === 'all') return true;if (range === 'today') return dayI === todayDayIndex;if (range === 'tomorrow') return dayI === (todayDayIndex + 1);if (range === 'week') return dayI >= todayDayIndex && dayI <= (todayDayIndex + 6);return true;}function applyFilters(){const q = (searchInput?.value || '').trim().toLowerCase();const comp = (compSelect?.value || '').trim();let shown = 0;cards.forEach(card => {const dayI = parseInt(card.getAttribute('data-dayi') || '0', 10);const hay = (card.getAttribute('data-search') || '');const c = (card.getAttribute('data-comp') || '');// ✅ filtro principal depende do modo
const okMain = (mode === 'day')
? (dayI === selectedDayI)          // ignora range
: dayInRange(dayI);                // ignora selectedDayI
const okSearch = (!q || hay.includes(q));const okComp = (!comp || c === comp);const show = okMain && okSearch && okComp;card.style.display = show ? '' : 'none';if (show) shown++;});if (empty) empty.style.display = (shown === 0) ? '' : 'none';}
// ===== Player modal (canais) =====
const modal = root.querySelector(".mt-player-modal");const iframe = root.querySelector(".mt-player-iframe");const channelBtns = Array.from(root.querySelectorAll(".mt-channel-btn"));function portalToBody(modal){if(modal && modal.parentNode !== document.body){document.body.appendChild(modal);}}const dialog = root.querySelector(".mt-player-dialog");const fsBtn  = root.querySelector(".mt-player-fs");// garante que o iframe permite fullscreen (mesmo que o HTML já tenha)
if (iframe) {iframe.setAttribute("allowfullscreen", "");const sb = iframe.getAttribute("sandbox") || "";if (!/allow-fullscreen/.test(sb)) {iframe.setAttribute("sandbox", (sb + " allow-fullscreen").trim());}}// Fit 16:9 no viewport, sem cortes
function fitPlayer() {if (!dialog) return;const pad = 28; // margem total (14+14)
const vw = Math.max(320, window.innerWidth  - pad);const vh = Math.max(320, window.innerHeight - pad);// tenta manter 16:9 dentro do viewport
let w = vw;let h = Math.floor(w * 9 / 16);if (h > vh) {h = vh;w = Math.floor(h * 16 / 9);}dialog.style.width  = w + "px";dialog.style.height = h + "px";}// Fullscreen controlado por nós (fora do iframe)
async function toggleFullscreen() {const el = dialog || iframe;if (!el) return;try {if (document.fullscreenElement) {await document.exitFullscreen();} else {// pede fullscreen do dialog (recomendado) ou do iframe
await el.requestFullscreen();}} catch (e) {// fallback iOS Safari (não existe fullscreen “real” igual)
// aqui não há muito a fazer além de tentar abrir em nova aba, se fosse permitido.
console.warn("Fullscreen falhou:", e);}}fsBtn?.addEventListener("click", toggleFullscreen);// sempre que abrir o modal: ajustar e “prender” a resize/orientation
function onOpenSizing() {fitPlayer();window.addEventListener("resize", fitPlayer, { passive: true });window.addEventListener("orientationchange", fitPlayer, { passive: true });// às vezes a UI do browser muda após abrir modal
setTimeout(fitPlayer, 120);setTimeout(fitPlayer, 350);}function onCloseSizing() {window.removeEventListener("resize", fitPlayer);window.removeEventListener("orientationchange", fitPlayer);// se estiver em fullscreen, sair quando fecha
if (document.fullscreenElement) {document.exitFullscreen().catch(() => {});}}// injeta no teu openPlayer/closePlayer
const _openPlayer = openPlayer;openPlayer = function(src){_openPlayer(src);onOpenSizing();};const _closePlayer = closePlayer;closePlayer = function(){onCloseSizing();_closePlayer();};function openPlayer(src){if(!modal || !iframe) return;portalToBody(modal); // 🔥 isto faz o modal ignorar o .app-zoom
iframe.src = src;modal.hidden = false;modal.setAttribute("aria-hidden", "false");document.documentElement.style.overflow = "hidden";document.body.style.overflow = "hidden";}function closePlayer(){if(!modal || !iframe) return;modal.hidden = true;modal.setAttribute("aria-hidden", "true");iframe.src = "about:blank";document.documentElement.style.overflow = "";document.body.style.overflow = "";}channelBtns.forEach(btn => {btn.addEventListener("click", () => {const src = btn.getAttribute("data-src") || "";if(src) openPlayer(src);});});modal?.addEventListener("click", (e) => {const t = e.target;if(t && t.getAttribute && t.getAttribute("data-close") === "1") closePlayer();});document.addEventListener("keydown", (e) => {if(e.key === "Escape" && modal && !modal.hidden) closePlayer();});function setActiveDay(index){if(!dias[index]) return;selectedDayI = index;dias.forEach(d => d.classList.remove('active'));dias[index].classList.add('active');scrollToIndex(index);}function activateRangeButton(rangeKey){qfBtns.forEach(b => b.classList.remove('is-active'));const btn = qfBtns.find(b => (b.getAttribute('data-range') || '') === rangeKey);if (btn) btn.classList.add('is-active');}// ✅ clique nos dias: entra em modo 'day' e ignora os filtros Hoje/Amanhã/Semana/Tudo
dias.forEach((dia, i) => {dia.addEventListener('click', () => {mode = 'day';setActiveDay(i);applyFilters();});});// prev/next também são "modo dia"
prevBtn?.addEventListener('click', () => {mode = 'day';if (selectedDayI > 0) setActiveDay(selectedDayI - 1);applyFilters();});nextBtn?.addEventListener('click', () => {mode = 'day';if (selectedDayI < dias.length - 1) setActiveDay(selectedDayI + 1);applyFilters();});// ✅ quick filters: entra em modo 'range' e ignora o dia selecionado
qfBtns.forEach(btn => {btn.addEventListener('click', () => {mode = 'range';range = btn.getAttribute('data-range') || 'today';activateRangeButton(range);// opcional: só para UX, mexer o calendário para o dia relevante (mas não filtra por ele)
if (range === 'today') setActiveDay(Math.min(todayDayIndex, dias.length - 1));else if (range === 'tomorrow') setActiveDay(Math.min(todayDayIndex + 1, dias.length - 1));else if (range === 'week') setActiveDay(Math.min(todayDayIndex, dias.length - 1));applyFilters();});});// search debounce
