bell.addEventListener('click', (e) => {e.preventDefault();e.stopPropagation();toggleDrop();});// clicar dentro do dropdown não fecha
drop.addEventListener('click', (e) => {e.stopPropagation();});// clicar fora fecha
document.addEventListener('click', () => closeDrop());// ESC fecha
document.addEventListener('keydown', (e) => {if (e.key === 'Escape') closeDrop();});});</script><script>document.addEventListener("DOMContentLoaded", () => {const bell = document.getElementById("notifBell");const dd   = document.getElementById("notifDropdown");if (!bell || !dd) return;const ajaxUrl = (window.MT && MT.ajaxUrl) ? MT.ajaxUrl : "/wp-admin/admin-ajax.php";async function markAllChecked() {const body = new URLSearchParams();body.set("action", "mark_notif_checked");const res = await fetch(ajaxUrl, {method: "POST",credentials: "same-origin",headers: { "Content-Type": "application/x-www-form-urlencoded; charset=UTF-8" },body: body.toString()
});return res.json().catch(() => null);}function hideBadge() {const badge = bell.querySelector(".notif-badge");if (badge) badge.remove();}function clearUncheckedStyles() {dd.querySelectorAll(".notif-item.unchecked").forEach(li => li.classList.remove("unchecked"));}bell.addEventListener("click", async () => {// ✅ só marca como lidas quando o dropdown está aberto
const isDropdownOpen = dd.classList.contains("is-open");if (!isDropdownOpen) return;try {const json = await markAllChecked();if (json && json.success) {hideBadge();clearUncheckedStyles();}} catch (e) {console.error("mark_notif_checked falhou:", e);}});});</script><script>document.addEventListener('DOMContentLoaded', () => {const dd   = document.getElementById('notifDropdown');const bell = document.getElementById('notifBell');const list = dd ? dd.querySelector('.notif-list') : null;if (!dd || !list || !bell) return;const ajaxUrl = (window.MT && MT.ajaxUrl) ? MT.ajaxUrl : (window.ajaxurl || "/wp-admin/admin-ajax.php");let loading = false;let done = false;function getPage(){ return parseInt(dd.getAttribute('data-page') || '0', 10); }function setPage(p){ dd.setAttribute('data-page', String(p)); }function totalAll(){ return parseInt(dd.getAttribute('data-all-counter') || '0', 10); }function loadedCount(){ return list.querySelectorAll('.notif-item').length; }function reachedEnd(){const all = totalAll();return all > 0 && loadedCount() >= all;}async function loadMore(){if (loading || done) return;if (reachedEnd()) { done = true; return; }loading = true;const nextPage = getPage() + 1;try{const body = new URLSearchParams();body.set('action','load_ajax_notif');body.set('id', dd.getAttribute('data-user-id'));body.set('page', String(nextPage));const res = await fetch(ajaxUrl, {method:'POST',credentials:'same-origin',headers:{ 'Content-Type':'application/x-www-form-urlencoded; charset=UTF-8' },body: body.toString()
});const json = await res.json().catch(() => null);if (!json || !json.success || !Array.isArray(json.data) || json.data.length === 0) {done = true;return;}const empty = list.querySelector('.notif-empty');if (empty) empty.remove();json.data.forEach(n => {const li = document.createElement('li');li.id = `notif-${n.id}`;li.className = 'notif-item' + (n.unchecked ? ' unchecked' : '');const poster = (n.poster_path || '').replace(/^\//,'');const href   = n.url || n.permalink || '#';li.innerHTML = `
<a href="${href}"><div class="notif-poster"><img src="https://image.tmdb.org/t/p/w600_and_h900_bestv2/${poster}" alt="" loading="lazy"></div><div class="notif-body"><div class="notif-row"><div class="notif-name">${n.post_title || ''}</div><div class="notif-date">${n.date_human || ''}</div></div>${n.episode_text ? `<div class="notif-sub">${n.episode_text}</div>` : ``}</div></a>`;list.appendChild(li);});setPage(nextPage);// ✅ se já chegou ao total, para
if (reachedEnd()) done = true;// ✅ se ainda não encheu (não criou scroll), tenta buscar mais
if (!done && dd.scrollHeight <= dd.clientHeight + 5) {loadMore();}} catch(e){console.error('load_ajax_notif falhou:', e);} finally {loading = false;}}dd.addEventListener('scroll', () => {const nearBottom = dd.scrollTop + dd.clientHeight >= dd.scrollHeight - 80;if (nearBottom) loadMore();});bell.addEventListener('click', () => {setTimeout(() => {if (dd.classList.contains('is-open')) {// ✅ ao abrir, tenta logo completar até ter scroll
if (!done && dd.scrollHeight <= dd.clientHeight + 5) loadMore();}}, 60);});});</script><section id="mtcal_FkVvG9" class="mt-football-modern"><div class="mt-channels-top"> <div class="mt-channels-title">📺 Canais</div> <div class="mt-channels-row"> <button type="button" class="mt-channel-btn" data-src="https://lovetier.bz/player/SPT1" aria-label="SportTV 1" title="SportTV 1"> <img class="mt-channel-icon" src="./Canais de Desporto – megatuga.io • assistir online HD &amp; downloads grátis_files/sporttvv1.png" alt="SportTV 1" loading="lazy"> </button> <button type="button" class="mt-channel-btn" data-src="https://lovetier.bz/player/SPT2" aria-label="SportTV 2" title="SportTV 2"> <img class="mt-channel-icon" src="./Canais de Desporto – megatuga.io • assistir online HD &amp; downloads grátis_files/sporttvv2.png" alt="SportTV 2" loading="lazy"> </button> <button type="button" class="mt-channel-btn" data-src="https://lovetier.bz/player/SPT3" aria-label="SportTV 3" title="SportTV 3"> <img class="mt-channel-icon" src="./Canais de Desporto – megatuga.io • assistir online HD &amp; downloads grátis_files/sporttvv3.png" alt="SportTV 3" loading="lazy"> </button> <button type="button" class="mt-channel-btn" data-src="https://lovetier.bz/player/SPT4" aria-label="SportTV 4" title="SportTV 4"> <img class="mt-channel-icon" src="./Canais de Desporto – megatuga.io • assistir online HD &amp; downloads grátis_files/sporttvv4.png" alt="SportTV 4" loading="lazy"> </button> <button type="button" class="mt-channel-btn" data-src="https://lovetier.bz/player/SPT5" aria-label="SportTV 5" title="SportTV 5"> <img class="mt-channel-icon" src="./Canais de Desporto – megatuga.io • assistir online HD &amp; downloads grátis_files/sporttvv5.png" alt="SportTV 5" loading="lazy"> </button> <button type="button" class="mt-channel-btn" data-src="https://lovetier.bz/player/SPT6" aria-label="SportTV 6" title="SportTV 6"> <img class="mt-channel-icon" src="./Canais de Desporto – megatuga.io • assistir online HD &am